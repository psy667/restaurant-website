{
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules":true,
        "arrowFunctions":true,
        "classes":true,
        "spread":true
      },
      "env": {
        "browser": true,
        "node": true,
        "es6": true
      }
    },
    "extends":"airbnb-base",
    "rules": {
        "semi": "error"
    }
}
